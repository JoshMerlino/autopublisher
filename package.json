        "dev": "concurrently \"npm:watch-*\"",
        "start": "node .",
        "build": "tsc"
    },
    "devDependencies": {
        "@types/node": "^20.3.2",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "concurrently": "^8.2.0",
        "eslint": "^8.43.0",
        "nodemon": "^2.0.22",
        "typescript": "^5.1.5",
        "typescript-eslint": "0.0.1-alpha.0"
    },
    "dependencies": {
        "chalk": "^5.0.1",
        "discord.js": "^14.11.0",
        "dotenv": "^16.3.1",
        "filestore-json": "^1.1.8"
    }
