        "concurrently": "^8.2.2",
        "eslint": "^8.57.0",
        "nodemon": "^3.1.4",
        "typescript": "^5.5.4",
        "typescript-eslint": "8.4.0"
    },
    "dependencies": {
        "chalk": "^5.0.1",
